  author = {Cutter, Susan L. and Schumann, Ronald L. and Emrich, Christopher T.},
  date = {2014-06-16},
  journaltitle = {Journal of Extreme Events},
  shortjournal = {J. of Extr. Even.},
  volume = {01},
