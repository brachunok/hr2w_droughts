@article{yang2020,
  title = {A Federated Pre-Event Community Resilience Approach for Assessing Physical and Social Sub-Systems: An Extreme Rainfall Case in {{Hong Kong}}},
  shorttitle = {A Federated Pre-Event Community Resilience Approach for Assessing Physical and Social Sub-Systems},
  author = {Yang, Yifan and Ng, S. Thomas and Zhou, Shenghua and Xu, Frank J and Li, Dezhi and Li, Hongyang},
  date = {2020-01-01},
  journaltitle = {Sustainable Cities and Society},
