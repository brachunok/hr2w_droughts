@article{underwood2018,
  title = {Climate Change Impacts on Hydrological Services in Southern {{California}}},
  author = {Underwood, Emma C. and Hollander, Allan D. and Flint, Lorraine E. and Flint, Alan L. and Safford, Hugh D.},
  date = {2018-12},
  journaltitle = {Environmental Research Letters},
  shortjournal = {Environ. Res. Lett.},
  volume = {13},
  number = {12},
