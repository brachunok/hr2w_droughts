  shorttitle = {Distributional {{Statistics}} of {{Municipal Water Use During Cape Town}}'s {{Drought}}},
  author = {Cook, Joseph and Br\"uhl, Johanna and Visser, Martine},
  date = {2021},
  journaltitle = {Water Resources Research},
  volume = {57},
