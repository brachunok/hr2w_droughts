@article{geressu2020,
  title = {Assessing {{River Basin Development Given Water}}-{{Energy}}-{{Food}}-{{Environment Interdependencies}}},
  author = {Geressu, Robel and Siderius, Christian and Harou, Julien J. and Kashaigili, Japhet and Pettinotti, Laetitia and Conway, Declan},
  date = {2020-08},
