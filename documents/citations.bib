@article{ciullo2020,
  title = {Efficient or {{Fair}}? Operationalizing {{Ethical Principles}} in {{Flood Risk Management}}: A {{Case Study}} on the {{Dutch}}-{{German Rhine}}},
  shorttitle = {Efficient or {{Fair}}?},
  author = {Ciullo, Alessio and Kwakkel, Jan H. and De Bruijn, Karin M. and Doorn, Neelke and Klijn, Frans},
