@article{scanlon2016,
  title = {Enhancing Drought Resilience with Conjunctive Use and Managed Aquifer Recharge in {{California}} and {{Arizona}}},
  author = {Scanlon, Bridget R. and Reedy, Robert C. and Faunt, Claudia C. and Pool, Donald and Uhlman, Kristine},
  date = {2016-03},
