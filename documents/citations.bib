@article{jafino2021,
  title = {Accounting for {{Multisectoral Dynamics}} in {{Supporting Equitable Adaptation Planning}}: A {{Case Study}} on the {{Rice Agriculture}} in the {{Vietnam Mekong Delta}}},
  shorttitle = {Accounting for {{Multisectoral Dynamics}} in {{Supporting Equitable Adaptation Planning}}},
  author = {Jafino, Bramka Arga and Kwakkel, Jan H. and Klijn, Frans and Dung, Nguyen Viet and van Delden, Hedwig and Haasnoot, Marjolijn and Sutanudjaja, Edwin H.},
  options = {useprefix=true},
  date = {2021},
  journaltitle = {Earth's Future},
  volume = {9},
