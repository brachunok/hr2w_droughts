  file = {/Users/brachuno/zotero/storage/DINP9XL2/Cutter and Derakhshan - 2020 - Temporal and spatial change in disaster resilience.pdf;/Users/brachuno/zotero/storage/KPB73EHD/17477891.2018.html}
}

