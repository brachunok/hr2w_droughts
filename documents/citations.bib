@article{yabe2019,
  title = {Mobile Phone Data Reveals the Importance of Pre-Disaster Inter-City Social Ties for Recovery after {{Hurricane Maria}}},
  author = {Yabe, Takahiro and Ukkusuri, Satish V. and C. Rao, P. Suresh},
  date = {2019-10-30},
  journaltitle = {Applied Network Science},
  shortjournal = {Appl Netw Sci},
